local function OSXJQFY_fake_script() -- EasySploit.KeybindScript 
	local script = Instance.new('LocalScript', EasySploit)

	local HotKey = Enum.KeyCode.Z-- Key to open Gui --
	local UIS = game:GetService("UserInputService")
	
	UIS.InputBegan:Connect(function(Key)
		if Key.KeyCode == HotKey then
			script.Parent.Enabled = not script.Parent.Enabled
		end
	end)
end
coroutine.wrap(OSXJQFY_fake_script)()
