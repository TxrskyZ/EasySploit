--Welcome, Synapseian or Krnlpsien.

pcall(function()
	game.StarterGui:SetCore("SendNotification", {
		Title = 'ES Updates 60% Done';
		Text = 'Created new features, check em out.';
		Duration = 15;
	})
	if not game.Players.LocalPlayer:IsInGroup(12052955) then
		coroutine.resume(coroutine.create(function()
			wait(10)
			game.StarterGui:SetCore("SendNotification", {
				Title = 'you are not in the discord??';
				Text = 'the script will still work but our server link was copied to your clipboard thx';
				Duration = 10;
			})
			setclipboard('https://discord.gg/esNVMgT9MF')
		end))
	end
end)

