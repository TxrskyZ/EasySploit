SetSpeed.Name = "SetSpeed"
SetSpeed.Parent = AdvancedSettings
SetSpeed.BackgroundColor3 = Color3.fromRGB(20, 20, 20)
SetSpeed.Position = UDim2.new(0.625628114, 0, 0.530864179, 0)
SetSpeed.Size = UDim2.new(0, 134, 0, 26)
SetSpeed.Font = Enum.Font.GothamSemibold
SetSpeed.Text = "Set Speed"
SetSpeed.TextColor3 = Color3.fromRGB(175, 175, 175)
SetSpeed.TextSize = 14.000
SetSpeed.MouseButton1Down:Connect(function()
if SetSpeedBox.Text == "10" then
	local plr = game.Players.LocalPlayer
	local char = plr.Character

	char.Humanoid.WalkSpeed = 10
	if SetSpeedBox.Text == "20" then
		local plr = game.Players.LocalPlayer
		local char = plr.Character

		char.Humanoid.WalkSpeed = 20
		if SetSpeedBox.Text == "30" then
			local plr = game.Players.LocalPlayer
			local char = plr.Character

				char.Humanoid.WalkSpeed = 30
				if SetSpeedBox.Text == "40" then
					local plr = game.Players.LocalPlayer
					local char = plr.Character

					char.Humanoid.WalkSpeed = 40
					
				end

		end
	end
end
end)

